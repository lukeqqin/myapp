{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index"
    },
    {
      "path": "pages/genealogy/index"
    },
    {
      "path": "pages/service/index",
      "style": {
        "backgroundColorTop": "#f2f2f3",
        "backgroundColorBottom": "#f2f2f3"
      }
    },
    {
      "path": "pages/user/index",
      "style": {
        "backgroundColorTop": "#ffffff",
        "backgroundColorBottom": "#f2f2f3"
      }
    },
    {
      "path": "pages/genealogy/add"
    }
  ],
  "globalStyle": {
    "backgroundColorTop": "#f2f2f3",
    "navigationBarTitleText": "",
    "navigationStyle": "custom"
    //"disableScroll": true
  },
  "uniIdRouter": {},
  "tabBar": {
    "color": "#cdcdcd",
    "selectedColor": "#6134af",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "/static/images/tab-bar/home.png",
        "selectedIconPath": "/static/images/tab-bar/home_active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/genealogy/index",
        "iconPath": "/static/images/tab-bar/genealogy.png",
        "selectedIconPath": "/static/images/tab-bar/genealogy_active.png",
        "text": "家谱"
      },
      {
        "pagePath": "pages/service/index",
        "iconPath": "/static/images/tab-bar/discover.png",
        "selectedIconPath": "/static/images/tab-bar/discover_active.png",
        "text": "发现"
      },
      {
        "pagePath": "pages/user/index",
        "iconPath": "/static/images/tab-bar/my.png",
        "selectedIconPath": "/static/images/tab-bar/my_active.png",
        "text": "我的"
      }
    ],
    "backgroundColor": "#f2f2f3"
  }
}